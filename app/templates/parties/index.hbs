<div class="container-fluid">
  <div class="panel col-md-12">
    Vous avez jou√© {{games_count}} jeux
  </div>
  <div class="col-md-9">
    Les partie du mois de {{month}}
    <table class="table table-bordered">
      <tbody>
      {{#each week in weeks}}
        <tr>
          {{#each day in week}}
            <td>
              {{ formatDateWith day "dd D" }} : {{day.parties.length}} partie
              <ul>
                {{#each p in day.parties }}
                  <li>{{p.game.name}}</li>
                  {{log g}}
                {{/each}}
              </ul>
            </td>
          {{/each}}
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
  <div class="col-md-3">
    <ul>
      {{#each}}
        <li>{{game.name}} a {{nbPlayer}}</li>
      {{/each}}
    </ul>
  </div>
</div>
